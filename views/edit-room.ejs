  <link rel="stylesheet" href="css/editor/source.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/keyboard.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/dialog.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/app.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/tools.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/button.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/select.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/panel.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/align_buttons.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/text.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/sponsors.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/zoom-dropdown.css" type="text/css"/>
