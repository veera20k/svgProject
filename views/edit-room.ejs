  <link rel="stylesheet" href="css/editor/color-tool.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/context-menu.css" type="text/css"/>
  <link rel="stylesheet" href="css/editor/shapelib.css" type="text/css"/>
