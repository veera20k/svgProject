<body class="loading">
<div id="method-draw" class="app">
<div id="rulers">
  <div id="ruler_corner"></div>
  <div id="ruler_x"></div>
  <div id="ruler_y"></div>
</div>

<div id="workarea">
<div id="svgcanvas">
</div>
</div>

